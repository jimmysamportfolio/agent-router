{
  "name": "agent-router",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "dev:db": "docker compose up -d",
    "dev:migrate": "npx tsx --env-file=.env.local src/scripts/migrate.ts",
    "dev:seed": "npx tsx --env-file=.env.local src/scripts/seed-policies.ts",
    "dev:full": "docker compose up -d --wait && npx tsx --env-file=.env.local src/scripts/migrate.ts && next dev",
    "dev:worker": "npx tsx --env-file=.env.local src/server/worker.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@google/genai": "^1.0.0",
    "@tanstack/react-query": "^5.90.21",
    "@trpc/client": "^11.10.0",
    "@trpc/next": "^11.10.0",
    "@trpc/react-query": "^11.10.0",
    "@trpc/server": "^11.10.0",
    "bullmq": "^5.69.3",
    "dotenv": "^16.4.7",
    "ioredis": "^5.9.3",
    "next": "16.1.6",
    "openai": "^6.22.0",
    "pg": "^8.18.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "tailwindcss": "^4",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  }
}
